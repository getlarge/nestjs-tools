{
  "name": "@s1seven/nestjs-tools-file-storage",
  "version": "0.6.3",
  "description": "File storage modules",
  "license": "Apache-2.0",
  "author": "getlarge <ed@getlarge.eu>",
  "homepage": "https://github.com/s1seven/nestjs-tools#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "publicConfig": {
    "access": "public"
  },
  "private": false,
  "types": "dist/index",
  "main": "dist/index",
  "directories": {
    "src": "src",
    "lib": "dist",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "cleanup": "rimraf -rf dist coverage",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "eslint -c ../../.eslintrc.js \"**/*.ts\"",
    "lint:fix": "eslint -c ../../.eslintrc.js \"**/*.ts\" --fix",
    "test": "jest",
    "test:cov": "rimraf -rf ./coverage && jest --ci --coverage --logHeapUsage",
    "scan:deps": "audit-ci --config ../../audit-ci.json",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.290.0",
    "@aws-sdk/lib-storage": "3.290.0",
    "reflect-metadata": "^0.1.13"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.8",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.8",
    "rxjs": "^7.5.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.1.6",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "dotenv": "^16.0.3"
  },
  "gitHead": "990d1604164e9bc5b61c93eca1a2d51416a0dfa6"
}

{
  "name": "@s1seven/nestjs-tools-file-storage",
  "version": "0.4.5",
  "description": "File storage modules",
  "license": "Apache-2.0",
  "author": "getlarge <ed@getlarge.eu>",
  "homepage": "https://github.com/s1seven/nestjs-tools#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "publicConfig": {
    "access": "public"
  },
  "private": false,
  "types": "dist/index",
  "main": "dist/index",
  "directories": {
    "src": "src",
    "lib": "dist",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "cleanup": "rimraf -rf dist coverage",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "eslint -c ../../.eslintrc.js \"**/*.ts\"",
    "lint:fix": "eslint -c ../../.eslintrc.js \"**/*.ts\" --fix",
    "test": "jest --ci --passWithNoTests",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.1",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.4.1",
    "aws-sdk": "^2.1093.0",
    "reflect-metadata": "^0.1.13"
  },
  "peerDependencies": {
    "rxjs": "^7.5.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6"
  }
}

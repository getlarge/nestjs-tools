{
  "name": "@s1seven/nestjs-tools-lock",
  "version": "0.4.4",
  "description": "Lock service",
  "author": "getlarge <ed@getlarge.eu>",
  "homepage": "https://github.com/s1seven/nestjs-tools/tree/main/packages/lock#readme",
  "license": "ISC",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "publicConfig": {
    "access": "public"
  },
  "private": false,
  "main": "dist/index",
  "types": "dist/index",
  "directories": {
    "dist": "dist",
    "templates": "templates",
    "src": "src",
    "test": "tests"
  },
  "files": [
    "dist",
    "templates"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s1seven/nestjs-tools.git"
  },
  "bugs": {
    "url": "https://github.com/s1seven/nestjs-tools/issues"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "cleanup": "rimraf -rf dist coverage",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "eslint -c ../../.eslintrc.js \"**/*.ts\"",
    "lint:fix": "eslint -c ../../.eslintrc.js \"**/*.ts\" --fix",
    "test": "jest",
    "test:cov": "rimraf -rf ./coverage && jest --ci --coverage --logHeapUsage",
    "scan:deps": "audit-ci --config ../../audit-ci.json",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "node-redis-warlock": "^1.0.2",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.4.6",
    "@nestjs/core": "^8.4.6",
    "rxjs": "^7.5.4"
  },
  "devDependencies": {
    "@nestjs/config": "^2.1.0",
    "@nestjs/testing": "^8.4.6",
    "@types/node": "^16.11.6",
    "@types/redis": "^2.8.31"
  }
}

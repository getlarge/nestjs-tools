{
  "name": "@s1seven/nestjs-tools-amqp-transport",
  "version": "0.4.1",
  "description": "Custom Nest Transport classes for AMQP",
  "license": "Apache-2.0",
  "author": "getlarge <ed@getlarge.eu>",
  "homepage": "https://github.com/s1seven/nestjs-tools#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "publicConfig": {
    "access": "public"
  },
  "private": false,
  "types": "dist/index",
  "main": "dist/index",
  "directories": {
    "src": "src",
    "lib": "dist",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "cleanup": "rimraf -rf dist coverage",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "eslint -c ../../.eslintrc.js \"**/*.ts\"",
    "lint:fix": "eslint -c ../../.eslintrc.js \"**/*.ts\" --fix",
    "test": "jest",
    "test:cov": "rimraf -rf ./coverage && jest --ci --coverage --logHeapUsage",
    "scan:deps": "audit-ci --config ../../audit-ci.json",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@s1seven/typed-event-emitter": "^0.1.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.8",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.8",
    "@nestjs/microservices": "^9.0.8",
    "amqp-connection-manager": "^4.1.3",
    "amqplib": "^0.10.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.0.8",
    "@types/amqplib": "^0.8.2",
    "@types/node": "^16.11.6",
    "mqtt": "^4.3.5"
  }
}
